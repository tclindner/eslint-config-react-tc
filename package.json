{
  "name": "eslint-config-react-tc",
  "version": "0.0.0",
  "description": "ESLint shareable config for React projects",
  "keywords": [
    "eslintconfig",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "eslint",
    "code style",
    "react"
  ],
  "homepage": "https://github.com/tclindner/eslint-config-react-tc",
  "bugs": {
    "url": "https://github.com/tclindner/eslint-config-react-tc/issues"
  },
  "author": "Thomas Lindner",
  "repository": {
    "type": "git",
    "url": "https://github.com/tclindner/eslint-config-react-tc.git"
  },
  "files": [
    "index.js",
    "rules",
    "CONTRIBUTING.md"
  ],
  "main": "index.js",
  "scripts": {
    "eslint": "eslint . --ignore-pattern 'test/fixtures/*' --format=node_modules/eslint-formatter-pretty",
    "lint": "npm run npmpackagejsonlint && npm run eslint",
    "npmpackagejsonlint": "npmPkgJsonLint .",
    "test": "jest"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-config-tc": "^24.0.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "is-plain-obj": "^3.0.0",
    "jest": "^29.6.1",
    "npm-package-json-lint": "^7.0.0",
    "npm-package-json-lint-config-tc": "^7.0.0",
    "prettier": "^3.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "license": "MIT"
}
