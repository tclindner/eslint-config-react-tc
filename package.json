{
  "name": "eslint-config-react-tc",
  "version": "4.0.0",
  "description": "ESLint shareable config for React projects",
  "keywords": [
    "eslintconfig",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "eslint",
    "code style",
    "react"
  ],
  "homepage": "https://github.com/tclindner/eslint-config-react-tc",
  "bugs": {
    "url": "https://github.com/tclindner/eslint-config-react-tc/issues"
  },
  "author": "Thomas Lindner",
  "repository": {
    "type": "git",
    "url": "https://github.com/tclindner/eslint-config-react-tc.git"
  },
  "files": [
    "index.js",
    "rules",
    "CONTRIBUTING.md"
  ],
  "main": "index.js",
  "scripts": {
    "eslint": "eslint . --format=node_modules/eslint-formatter-pretty",
    "lint": "npm run npmpackagejsonlint && npm run eslint",
    "npmpackagejsonlint": "npmPkgJsonLint .",
    "test": "jest"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-tc": "^8.0.1",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "is-plain-obj": "^2.0.0",
    "jest": "^24.9.0",
    "npm-package-json-lint": "^4.0.0-beta.3",
    "npm-package-json-lint-config-tc": "^2.2.0",
    "prettier": "^1.18.2"
  },
  "peerDependencies": {
    "eslint": "^6.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
