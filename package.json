{
  "name": "eslint-config-react-tc",
  "version": "6.0.0",
  "description": "ESLint shareable config for React projects",
  "keywords": [
    "eslintconfig",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "eslint",
    "code style",
    "react"
  ],
  "homepage": "https://github.com/tclindner/eslint-config-react-tc",
  "bugs": {
    "url": "https://github.com/tclindner/eslint-config-react-tc/issues"
  },
  "author": "Thomas Lindner",
  "repository": {
    "type": "git",
    "url": "https://github.com/tclindner/eslint-config-react-tc.git"
  },
  "files": [
    "index.js",
    "rules",
    "CONTRIBUTING.md"
  ],
  "main": "index.js",
  "scripts": {
    "eslint": "eslint . --format=node_modules/eslint-formatter-pretty",
    "lint": "npm run npmpackagejsonlint && npm run eslint",
    "npmpackagejsonlint": "npmPkgJsonLint .",
    "test": "jest"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-tc": "^12.1.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "is-plain-obj": "^2.1.0",
    "jest": "^25.5.4",
    "npm-package-json-lint": "^5.1.0",
    "npm-package-json-lint-config-tc": "^4.0.0",
    "prettier": "^2.0.5"
  },
  "peerDependencies": {
    "eslint": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.0.0",
    "prettier": "^2.0.0"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
